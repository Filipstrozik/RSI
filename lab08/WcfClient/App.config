<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IDatabaseService" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IDatabaseService">
                    <security mode="None" />
                </binding>
            </wsHttpBinding>
        </bindings>
		<client>
      <endpoint address="http://127.0.0.1:10000/DatabaseService/endpoint1"
          binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IDatabaseService"
          contract="WcfService.ICalculator" name="BasicHttpBinding_ICalculator" />
      <endpoint address="http://127.0.0.1:10000/DatabaseService/endpoint2"
          binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IDatabaseService"
          contract="WcfService.IDatabaseService" name="WSHttpBinding_ICalculator" />
  </client>
    </system.serviceModel>
</configuration>