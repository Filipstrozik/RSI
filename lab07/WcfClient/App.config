<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICalculator" />
            </basicHttpBinding>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_ISuperCalc" />
            </wsDualHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_ICalculator">
                    <security mode="None" />
                </binding>
            </wsHttpBinding>
        </bindings>
		<client>
      <endpoint address="http://192.168.0.80:10000/MyCalculator/endpoint1"
          binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICalculator"
          contract="WcfService.ICalculator" name="BasicHttpBinding_ICalculator" />
      <endpoint address="http://192.168.0.80:10000/MyCalculator/endpoint2"
          binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_ICalculator"
          contract="WcfService.ICalculator" name="WSHttpBinding_ICalculator" />
      <endpoint address="http://192.168.0.80:10000/MySuperCalculator/endpoint3"
          binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ISuperCalc"
          contract="CallbackService.ISuperCalc" name="WSDualHttpBinding_ISuperCalc">
          <identity>
              <userPrincipalName value="HUAWEI\filip" />
          </identity>
      </endpoint>
  </client>
    </system.serviceModel>
</configuration>